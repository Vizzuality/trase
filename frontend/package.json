{
  "name": "trase-frontend",
  "version": "1.0.0",
  "description": "Frontend for trase.earth",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.dev.js",
    "build": "npm run webpack:prod && cp -a public/. dist/",
    "test": "jest ./scripts --colors",
    "webpack:prod": "NODE_ENV='production' webpack --config config/webpack.config.prod.js",
    "lint-js": "eslint -c .eslintrc.json --ext .jsx,.js --fix",
    "lint-sass": "sass-lint -c .sass-lint.yml -v -q",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore --write",
    "precommit": "lint-staged",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "lint-staged": {
    "*.scss": "lint-sass",
    "*.{js,jsx}": [
      "prettier",
      "lint-js",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vizzuality/trase.git"
  },
  "author": "Vizzuality",
  "license": "MIT",
