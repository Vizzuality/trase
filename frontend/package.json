{
  "name": "trase-frontend",
  "version": "1.0.0",
  "description": "Frontend for trase.earth",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.config.dev.js",
    "start:staging": "webpack-dev-server --config config/webpack.config.staging.js",
    "build": "yarn webpack:prod && cp -a public/. dist/",
    "ci": "start-server-and-test start:staging http-get://localhost:8081 test",
    "test": "yarn unit && yarn e2e",
    "unit": "jest --config ./jest.config.js --colors --runInBand",
    "e2e": "jest --config ./jest.e2e.config.js --colors --runInBand --testMatch='**/?(*.)+(spec|test).e2e.js?(x)'",
    "webpack:prod": "NODE_ENV='production' webpack --config config/webpack.config.prod.js",
    "eslint": "eslint -c .eslintrc.json --ext .jsx,.js scripts/**/* --fix",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore --write 'scripts/**/*'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "named-maps": "node ./gis/cartodb/named-maps.js",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.scss": "sass-lint -c .sass-lint.yml -v -q",
    "*.{js,jsx}": [
      "prettier --config .prettierrc --ignore-path .prettierignore --write",
      "eslint -c .eslintrc.json --ext .jsx,.js --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vizzuality/trase.git"
  },
  "author": "Vizzuality",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vizzuality/trase/issues"
  },
  "homepage": "https://github.com/Vizzuality/trase#readme",
