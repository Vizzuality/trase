<div class="js-infowindow-title title"><%= title %></div>
<% Object.keys(values).forEach(function (i) { %>
<div class="js-infowindow-value-<%= i %> value">
  <div class="title"><%= values[i].title %></div>
  <div class="data">
    <%= values[i].value %>
    <% if (values[i].unit && values[i].unit !== 'Number') { %>
    <span class="unit">
          <%= values[i].unit %>
        </span>
    <% } %>
  </div>
</div>
<% }); %>
