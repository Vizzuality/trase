<% Object.keys(basemaps).forEach(function (i) { %>
<li class="map-sidebar-group-item js-map-sidebar-group-item -no-self-cancel" data-value="<%= i %>" >
    <div class="basemap-thumb" style="background-image: url('<%= basemaps[i].thumbnail %>');" >
        <div class="basemap-name" ><%= basemaps[i].title %></div >
    </div >
    <div class="radios" >
        <div class="radio-btn" ></div >
    </div >
</li >
<% }); %>
