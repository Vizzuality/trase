<% posts.forEach(function(post) { %>
<div class="post" >
    <div class="post-wrapper" >
        <figure class="post-image"
                <% if (post['imageUrl']) { %>
                style="background-image: url('<%= post['imageUrl'] %>')"
                <% } %>
        ></figure >
        <figcaption >
            <% if (post['title']) { %>
            <% if (post['completePostUrl']) { %>
            <a href="<%= post['completePostUrl'] %>" class="post-title" ><%= post['title'] %></a >
            <% } else { %>
            <%= post['title'] %>
            <% } %>
            <% } %>
        </figcaption >
    </div >
</div >
<% }) %>
