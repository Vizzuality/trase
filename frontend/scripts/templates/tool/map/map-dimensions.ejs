<% groups.forEach(function(group) { %>
<div class="map-sidebar-group js-map-sidebar-group -expandable" data-group-id="<%= group.group.id %>" >
    <div class="map-sidebar-group-title js-map-sidebar-group-title" >
        <%= group.group.name %>
    </div >
    <ul class="map-sidebar-group-items" >
        <% group.dimensions.forEach(function(dimension) { %>
        <li class="map-sidebar-group-item map-choropeth-layer-item js-map-sidebar-group-item" data-dimension-uid="<%= dimension.uid %>" >
            <div class="label" >
                <%= dimension.name %>
                <% if (dimension.unit) { %>
                (<%= dimension.unit %>)
                <% } %>
            </div >
            <div class="actions" >
                <% if (dimension.disabledYearRangeReasonText !== undefined) { %>
                <svg class="icon icon-layer-info js-tooltip"
                     data-tooltip-text="<%= dimension.disabledYearRangeReasonText %>" >
                    <use xlink:href="#icon-warning" ></use >
                </svg >
                <% } %>
                <% if (dimension.description) { %>
                <svg class="icon icon-layer-info js-tooltip" data-tooltip-text="<%= dimension.description %>" >
                    <use xlink:href="#icon-layer-info" ></use >
                </svg >
                <% } %>
                <div class="radio-btn" ></div >
            </div >
        </li >
        <% }) %>
    </ul >
</div >
<% }) %>
