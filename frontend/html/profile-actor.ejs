 <div class="loading-wrapper">
   <nav id="nav"></nav>
    <div class="l-profile-actor js-profiles">
      <div class="js-loading">
        <div class="c-spinner"></div>
      </div>
      <div class="js-wrap is-hidden">
        <div class="c-overall-info">
          <div class="row">
            <div class="small-12 columns">
              <div class="dropdown-element">
                <div class="c-dropdown -big">
                  <span class="dropdown-label js-legend">-</span>
                  <span class="dropdown-title js-name">-</span>
                  <ul class="dropdown-list is-hidden">
                  </ul>
                </div>
              </div>
              <div class="dropdown-element">
                <div class="c-dropdown -big">
                  <span class="dropdown-label">Country</span>
                  <span class="dropdown-title js-country">-</span>
                  <ul class="dropdown-list is-hidden">
                  </ul>
                </div>
              </div>
              <div class="dropdown-element">
                <div class="c-dropdown -big">
                  <span class="dropdown-label">Commodity</span>
                  <span class="dropdown-title">Soy</span>
                  <ul class="dropdown-list">
                  </ul>
                </div>
              </div>
              <div id="year-dropdown" class="dropdown-element">
              </div>
            </div>
            <div class="small-12 columns">
              <div class="stat-item zero-deforestation-commitment js-zero-deforestation-commitment">
                <div class="legend">ZERO DEFORESTATION COMMITMENT<span id="zero-deforestation-tooltip"></span></div>
                <div class="value is-hidden" data-value="yes"><svg class="icon icon-check"><use xlink:href="#icon-check-circle"/></svg>Yes</div>
                <div class="value is-hidden" data-value="no"><svg class="icon icon-no"><use xlink:href="#icon-no-circle"/></svg>No</div>
              </div>
              <div class="stat-item">
                <div class="legend">FOREST 500 SCORE<span id="forest-500-tooltip"></span></div>
                <div class="value forest-500-score js-forest-500-score">
                  <svg class="icon circle-icon" data-value="1"><use xlink:href="#icon-circle-empty"/></svg>
                  <svg class="icon circle-icon" data-value="2"><use xlink:href="#icon-circle-empty"/></svg>
                  <svg class="icon circle-icon" data-value="3"><use xlink:href="#icon-circle-empty"/></svg>
                  <svg class="icon circle-icon" data-value="4"><use xlink:href="#icon-circle-empty"/></svg>
                  <svg class="icon circle-icon" data-value="5"><use xlink:href="#icon-circle-empty"/></svg>
                </div>
              </div>
            </div>
            <div class="small-12 large-<%=printMode ? '12' : '7' %> columns">
              <div class="summary js-summary-text"></div>
            </div>
          </div>
        </div>

        <div class="c-anchor-buttons hide-for-small">
          <div class="row">
            <div class="small-4 columns">
              <a href="#profile-content-anchor" class="c-link-buttons -with-arrow js-link-profile">
                <img src="/images/profiles/profile-main-option-1.svg"/>
                <span class="js-link-button-name">-</span>
              </a>
            </div>
            <div class="small-4 columns">
              <a class="c-link-buttons js-link-map">
                <img src="/images/profiles/profile-main-option-2.svg"/>
                <span>PRODUCTION REGIONS</span>
                <svg class="icon icon-external-link"><use xlink:href="#icon-external-link"></use></svg>
              </a>
            </div>
            <div class="small-4 columns">
              <a class="c-link-buttons js-link-supply-chain">
                <img src="/images/profiles/profile-main-option-3.svg"/>
                <span>SUPPLY CHAIN ACTORS</span>
                <svg class="icon icon-external-link"><use xlink:href="#icon-external-link"></use></svg>
              </a>
            </div>
          </div>
        </div>

        <div id="profile-content-anchor" class="profile-content-anchor"></div>

        <section class="c-top-map is-page-break-always">
          <div class="row">
            <div class="small-12 medium-<%=printMode ? '6' : '8' %> columns">
              <h3 class="title -small js-top-map-title">-</h3>
            </div>
            <div class="small-<%=printMode ? '6' : '4' %> columns hide-for-small">
              <div class="js-destination-legend c-map-legend-choro"></div>
            </div>
          </div>
          <div class="row">
            <div class="small-12 medium-6 columns">
              <div class="c-line js-top-destination"></div>
            </div>
            <div class="small-6 columns js-top-destination-map hide-for-small"></div>
          </div>
        </section>

        <section class="c-top-municipalities is-page-break-always">
          <div class="row">
            <div class="js-top-municipalities-title-container small-12 medium-<%=printMode ? '6' : '8' %> columns">
            </div>
            <div class="small-<%=printMode ? '6' : '4' %> columns hide-for-small">
              <div class="js-source-legend c-map-legend-choro"></div>
            </div>
          </div>
          <div class="row">
            <div class="small-12 medium-7 columns">
              <div class="c-line js-top-municipalities"></div>
            </div>
            <div class="small-5 columns js-top-municipalities-map hide-for-small"></div>
          </div>
        </section>

        <section class="c-area-table is-page-break-always">
          <div class="row">
            <div class="small-12 columns">
              <div class="js-sustainability-table"></div>
            </div>
          </div>
        </section>

        <section class="c-scatterplot-container is-page-break-always">
          <div class="row">
            <div class="small-12 columns">
              <div class="js-scatterplot-container"></div>
            </div>
          </div>
        </section>

      </div>
      <div class="c-error-message js-error-message is-hidden">
        <p class="message js-message">Data not found</p>
      </div>
    </div>
    <div class="js-infowindow c-info-tooltip is-hidden"></div>
    <footer id="footer" class="c-footer"></footer>
   <%= feedback %>
 </div>
