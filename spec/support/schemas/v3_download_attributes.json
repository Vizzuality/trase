{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "indicators": {
      "$id": "/properties/indicators",
      "type": "array",
      "items": {
        "$id": "/properties/indicators/items",
        "type": "object",
        "properties": {
          "name": {
            "$id": "/properties/indicators/items/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
              "Volume"
            ]
          },
          "unit": {
            "$id": "/properties/indicators/items/properties/unit",
            "type": ["string", "null"],
            "title": "The Unit Schema ",
            "default": "",
            "examples": [
              "Tn"
            ]
          },
          "unitType": {
            "$id": "/properties/indicators/items/properties/unitType",
            "type": ["string", "null"],
            "title": "The Unittype Schema ",
            "default": "",
            "examples": [
              "volume"
            ]
          },
          "years": {
            "$id": "/properties/indicators/items/properties/years",
            "type": ["array", "null"],
            "items": {
              "$id": "/properties/indicators/items/properties/years/items",
              "type": "integer",
              "title": "The 0th Schema ",
              "default": 0,
              "examples": [
                2010,
                2011,
                2012,
                2013,
                2014,
                2015
              ]
            }
          },
          "frontendName": {
            "$id": "/properties/indicators/items/properties/frontendName",
            "type": "string",
            "title": "The Frontendname Schema ",
            "default": "",
            "examples": [
              "SOY_EQUIVALENT_TONS"
            ]
          },
          "indicatorType": {
            "$id": "/properties/indicators/items/properties/indicatorType",
            "type": "string",
            "title": "The Indicatortype Schema ",
            "default": "",
            "examples": [
              "Quant"
            ]
          },
          "filterOptions": {
            "$id": "/properties/indicators/items/properties/filterOptions",
            "type": "object",
            "properties": {
              "ops": {
                "$id": "/properties/indicators/items/properties/filterOptions/properties/ops",
                "type": "array",
                "items": {
                  "$id": "/properties/indicators/items/properties/filterOptions/properties/ops/items",
                  "type": "string",
                  "title": "The 0th Schema ",
                  "default": "",
                  "examples": [
                    "eq",
                    "lt",
                    "gt"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}