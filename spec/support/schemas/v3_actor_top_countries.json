{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "included_years": {
          "$id": "/properties/data/properties/included_years",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/included_years/items",
            "type": "integer",
            "title": "The 0th Schema ",
            "default": 0,
            "examples": [
              2010,
              2011,
              2012,
              2013,
              2014,
              2015
            ]
          }
        },
        "buckets": {
          "$id": "/properties/data/properties/buckets",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/buckets/items",
            "type": "integer",
            "title": "The 0th Schema ",
            "default": 0,
            "examples": [
              5000,
              100000,
              300000,
              1000000
            ]
          }
        },
        "lines": {
          "$id": "/properties/data/properties/lines",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/lines/items",
            "type": "object",
            "properties": {
              "name": {
                "$id": "/properties/data/properties/lines/items/properties/name",
                "type": "string",
                "title": "The Name Schema ",
                "default": "",
                "examples": [
                  "CHINA"
                ]
              },
              "geo_id": {
                "$id": "/properties/data/properties/lines/items/properties/geo_id",
                "type": "string",
                "title": "The Geo_id Schema ",
                "default": "",
                "examples": [
                  "CN"
                ]
              },
              "values": {
                "$id": "/properties/data/properties/lines/items/properties/values",
                "type": "array",
                "items": {
                  "$id": "/properties/data/properties/lines/items/properties/values/items",
                  "type": "number",
                  "title": "The 0th Schema ",
                  "default": 0,
                  "examples": [
                    2621207.75,
                    3501386.5,
                    2998336.75,
                    3475022.75,
                    4101003.75,
                    5785085.5
                  ]
                }
              },
              "value9": {
                "$id": "/properties/data/properties/lines/items/properties/value9",
                "type": "integer",
                "title": "The Value9 Schema ",
                "default": 0,
                "examples": [
                  null
                ]
              }
            }
          }
        },
        "unit": {
          "$id": "/properties/data/properties/unit",
          "type": "string",
          "title": "The Unit Schema ",
          "default": "",
          "examples": [
            "t"
          ]
        },
        "style": {
          "$id": "/properties/data/properties/style",
          "type": "object",
          "properties": {
            "type": {
              "$id": "/properties/data/properties/style/properties/type",
              "type": "string",
              "title": "The Type Schema ",
              "default": "",
              "examples": [
                "line-points"
              ]
            },
            "style": {
              "$id": "/properties/data/properties/style/properties/style",
              "type": "string",
              "title": "The Style Schema ",
              "default": "",
              "examples": [
                "line-pink-with-points"
              ]
            }
          }
        }
      }
    }
  }
}
