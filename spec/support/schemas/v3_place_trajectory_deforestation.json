{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft/schema##",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "included_years": {
          "$id": "/properties/data/properties/included_years",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/included_years/items",
            "type": "integer",
            "title": "The 0th Schema ",
            "default": 0,
            "examples": [
              2001,
              2002,
              2003,
              2004,
              2005,
              2006,
              2007,
              2008,
              2009,
              2010,
              2011,
              2012,
              2013,
              2014,
              2015
            ]
          }
        },
        "unit": {
          "$id": "/properties/data/properties/unit",
          "type": "string",
          "title": "The Unit Schema ",
          "default": "",
          "examples": [
            "ha"
          ]
        },
        "lines": {
          "$id": "/properties/data/properties/lines",
          "type": "array",
          "items": {
            "$id": "/properties/data/properties/lines/items",
            "type": "object",
            "properties": {
              "name": {
                "$id": "/properties/data/properties/lines/items/properties/name",
                "type": "string",
                "title": "The Name Schema ",
                "default": "",
                "examples": [
                  "Soy deforestation"
                ]
              },
              "legend_name": {
                "$id": "/properties/data/properties/lines/items/properties/legend_name",
                "type": "string",
                "title": "The Legend_name Schema ",
                "default": "",
                "examples": [
                  "Soy deforestation"
                ]
              },
              "legend_tooltip": {
                "$id": "/properties/data/properties/lines/items/properties/legend_tooltip",
                "type": "string",
                "title": "The Legend_tooltip Schema ",
                "default": "",
                "examples": [
                  "Annual deforestation due to direct conversion for soy (ha) (currently only in Cerrado biome, 2010-2013). Calculated by crossing per-pixel annual deforestation alerts and soy crop maps."
                ]
              },
              "type": {
                "$id": "/properties/data/properties/lines/items/properties/type",
                "type": "string",
                "title": "The Type Schema ",
                "default": "",
                "examples": [
                  "area"
                ]
              },
              "style": {
                "$id": "/properties/data/properties/lines/items/properties/style",
                "type": "string",
                "title": "The Style Schema ",
                "default": "",
                "examples": [
                  "area-pink"
                ]
              },
              "values": {
                "$id": "/properties/data/properties/lines/items/properties/values",
                "type": "array",
                "items": {
                  "$id": "/properties/data/properties/lines/items/properties/values/items",
                  "type": ["number", "null"],
                  "title": "The 0th Schema ",
                  "default": null,
                  "examples": [
                    null,
                    null,
                    7848.1201171875,
                    2810.31005859375,
                    1378.219970703125,
                    691.8699951171875,
                    1060.1700439453125,
                    1356.949951171875,
                    1232.68994140625,
                    743.75,
                    640.0499877929688,
                    451.67999267578125,
                    382.489990234375,
                    null,
                    null
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}
