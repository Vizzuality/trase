{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "array",
      "items": {
        "$id": "/properties/data/items",
        "type": "object",
        "properties": {
          "name": {
            "$id": "/properties/data/items/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
              "Environmental indicators"
            ]
          },
          "included_columns": {
            "$id": "/properties/data/items/properties/included_columns",
            "type": "array",
            "items": {
              "$id": "/properties/data/items/properties/included_columns/items",
              "type": "object",
              "properties": {
                "name": {
                  "$id": "/properties/data/items/properties/included_columns/items/properties/name",
                  "type": "string",
                  "title": "The Name Schema ",
                  "default": "",
                  "examples": [
                    "Territorial deforestation"
                  ]
                },
                "unit": {
                  "$id": "/properties/data/items/properties/included_columns/items/properties/unit",
                  "type": "string",
                  "title": "The Unit Schema ",
                  "default": "",
                  "examples": [
                    "Ha"
                  ]
                },
                "tooltip": {
                  "$id": "/properties/data/items/properties/included_columns/items/properties/tooltip",
                  "type": ["string", "null"],
                  "title": "The Tooltip Schema ",
                  "default": "",
                  "examples": [
                    "Total deforestation per municipality for a given year (ha). Calculated only for forested and wooded biomes - the Amazon, Cerrado and Atlantic Forest. Clearance of native vegetation in BrazilÂ´s other biomes (Caatinga, Pantanal and Pampas) is not considered as deforestation in this indicator."
                  ]
                }
              }
            }
          },
          "rows": {
            "$id": "/properties/data/items/properties/rows",
            "type": "array",
            "items": {
              "$id": "/properties/data/items/properties/rows/items",
              "type": "object",
              "properties": {
                "name": {
                  "$id": "/properties/data/items/properties/rows/items/properties/name",
                  "type": "string",
                  "title": "The Name Schema ",
                  "default": "",
                  "examples": [
                    "Score"
                  ]
                },
                "have_unit": {
                  "$id": "/properties/data/items/properties/rows/items/properties/have_unit",
                  "type": "boolean",
                  "title": "The Have_unit Schema ",
                  "default": false,
                  "examples": [
                    true
                  ]
                },
                "values": {
                  "$id": "/properties/data/items/properties/rows/items/properties/values",
                  "type": "array",
                  "items": {
                    "$id": "/properties/data/items/properties/rows/items/properties/values/items",
                    "type": ["number", "null"],
                    "title": "The 0th Schema ",
                    "default": 0,
                    "examples": [
                      70,
                      null,
                      null,
                      null,
                      2,
                      45.41475296020508
                    ]
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
