{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "node_name": {
          "$id": "/properties/data/properties/node_name",
          "type": "string",
          "title": "The Node_name Schema ",
          "default": "",
          "examples": [
            "CARGILL"
          ]
        },
        "column_name": {
          "$id": "/properties/data/properties/column_name",
          "type": "string",
          "title": "The Column_name Schema ",
          "default": "",
          "examples": [
            "EXPORTER"
          ]
        },
        "country_name": {
          "$id": "/properties/data/properties/country_name",
          "type": "string",
          "title": "The Country_name Schema ",
          "default": "",
          "examples": [
            "BRAZIL"
          ]
        },
        "country_geo_id": {
          "$id": "/properties/data/properties/country_geo_id",
          "type": "string",
          "title": "The Country_geo_id Schema ",
          "default": "",
          "examples": [
            "BR"
          ]
        },
        "zero_deforestation_link": {
          "$id": "/properties/data/properties/zero_deforestation_link",
          "type": "string",
          "title": "The Zero_deforestation_link Schema ",
          "default": "",
          "examples": [
            "HTTP://WWW.CARGILL.COM/NEWS/ISSUES/DEFORESTATION/INDEX.JSP"
          ]
        },
        "rtrs_member": {
          "$id": "/properties/data/properties/rtrs_member",
          "type": "string",
          "title": "The Rtrs_member Schema ",
          "default": "",
          "examples": [
            "YES"
          ]
        },
        "zero_deforestation": {
          "$id": "/properties/data/properties/zero_deforestation",
          "type": "string",
          "title": "The Zero_deforestation Schema ",
          "default": "",
          "examples": [
            "YES"
          ]
        },
        "supply_change": {
          "$id": "/properties/data/properties/supply_change",
          "type": "string",
          "title": "The Supply_change Schema ",
          "default": "",
          "examples": [
            "YES"
          ]
        },
        "supply_change_link": {
          "$id": "/properties/data/properties/supply_change_link",
          "type": "string",
          "title": "The Supply_change_link Schema ",
          "default": "",
          "examples": [
            "HTTP://SUPPLY-CHANGE.ORG/COMPANY/CARGILL"
          ]
        },
        "soy_": {
          "$id": "/properties/data/properties/soy_",
          "type": "number",
          "title": "The Soy_ Schema ",
          "default": 0,
          "examples": [
            8872367
          ]
        },
        "forest_500": {
          "$id": "/properties/data/properties/forest_500",
          "type": "number",
          "title": "The Forest_500 Schema ",
          "default": 0,
          "examples": [
            8872367
          ]
        },
        "summary": {
          "$id": "/properties/data/properties/summary",
          "type": "string",
          "title": "The Summary Schema ",
          "default": "",
          "examples": [
            "Cargill was the 2nd largest exporter of soy in BRAZIL in 2015, accounting for 8.9 million tons. This is a 30% increase vs the previous year. As an exporter, Cargill sources from 829 municipalities, or 40% of the soy production municipalities. The main destination of the soy exported by Cargill is China, accounting for 65% of the total."
          ]
        }
      }
    }
  }
}
