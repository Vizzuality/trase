{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "data": {
      "$id": "/properties/data",
      "type": "array",
      "items": {
        "$id": "/properties/data/items",
        "type": "object",
        "properties": {
          "id": {
            "$id": "/properties/data/items/properties/id",
            "type": "integer",
            "title": "The Id Schema ",
            "default": 0,
            "examples": [
              6035
            ]
          },
          "mainId": {
            "$id": "/properties/data/items/properties/mainId",
            "type": [
              "integer",
              "null"
            ],
            "title": "The Main Id Schema ",
            "default": null,
            "examples": [
              6035
            ]
          },
          "name": {
            "$id": "/properties/data/items/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
              "SHARIKAT AL-AKKAD LEL TEJARAT AL-AMMEH"
            ]
          },
          "nodeType": {
            "$id": "/properties/data/items/properties/nodeType",
            "type": "string",
            "title": "The Nodetype Schema ",
            "default": "",
            "examples": [
              "IMPORTER"
            ]
          },
          "contextId": {
            "$id": "/properties/data/items/properties/contextId",
            "type": "integer",
            "title": "The Contextid Schema ",
            "default": 0,
            "examples": [
              3
            ]
          },
          "profile": {
            "$id": "/properties/data/items/properties/profile",
            "type": "null",
            "title": "The Profile Schema ",
            "default": null,
            "examples": [
              null
            ]
          },
          "isSubnational": {
            "$id": "/properties/data/items/properties/isSubnational",
            "type": "boolean",
            "title": "The isSubnational Schema ",
            "default": false,
            "examples": [
              true,
              false
            ]
          }
        }
      }
    }
  }
}
