{
  "definitions": {},
  "$schema": "http://json-schema.org/draft/schema##",
  "$id": "http://example.com/root.json",
  "type": "object",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "items": {
        "$id": "#/properties/data/items",
        "type": "object",
        "required": [
          "id",
          "mainId",
          "name",
          "nodeType",
          "contextId",
          "profile",
          "isSubnational",
          "years"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/data/items/properties/id",
            "type": "integer"
          },
          "mainId": {
            "$id": "#/properties/data/items/properties/mainId",
            "type": ["integer", "null"]
          },
          "name": {
            "$id": "#/properties/data/items/properties/name",
            "type": "string",
            "pattern": "^(.*)$"
          },
          "nodeType": {
            "$id": "#/properties/data/items/properties/nodeType",
            "type": "string",
            "pattern": "^(.*)$"
          },
          "contextId": {
            "$id": "#/properties/data/items/properties/contextId",
            "type": "integer"
          },
          "profile": {
            "$id": "#/properties/data/items/properties/profile",
            "type": ["string", "null"],
            "pattern": "^(.*)$"
          },
          "isSubnational": {
            "$id": "#/properties/data/items/properties/isSubnational",
            "type": "boolean"
          },
          "years": {
            "$id": "#/properties/data/items/properties/years",
            "type": "array",
            "items": {
              "$id": "#/properties/data/items/properties/years/items",
              "type": "integer"
            }
          }
        }
      }
    }
  }
}
